<p id="notice"><%= notice %></p>

<h1>Technicians</h1>

<table class="table">
  <thead>
    <tr scope="row">
      <th scope="col">Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @technicians.each do |technician| %>
      <tr scope="row">
        <td><%= technician.name %></td>
        <td><%= link_to 'Show', technician, class: 'btn btn-primary' %></td>
        <td><%= link_to 'Edit', edit_technician_path(technician), class: 'btn btn-primary' %></td>
        <td><%= link_to 'Destroy', technician, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Technician', new_technician_path, class: 'btn btn-success' %>

<%= link_to 'Back to Work Orders', work_orders_path, class: 'btn btn-warning' %>
